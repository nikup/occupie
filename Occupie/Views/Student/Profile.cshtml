@model Occupie.Models.Student

@{
    ViewBag.Title = "Profile";
}

<!-- portfolio content-->
<div id="portfolio-content" class="cf">

    <ul id="filter-buttons">
        <li><a href="#" data-filter="*" class="selected">всичко</a></li>
        <li><a href="#" data-filter=".main">основна информация</a></li>
        <li><a href="#" data-filter=".edu">образование</a></li>
        <li><a href="#" data-filter=".experience">опит</a></li>
        <li><a href="#" data-filter=".personal">лична информация</a></li>
    </ul>

    <!-- Filter container -->
    <div id="filter-container" class="cf">

        @Html.Partial("_PicturePartial")

        @Html.Partial("../Job/_JobsPartial")

        @Html.Partial("_WorkDetailsPartial")


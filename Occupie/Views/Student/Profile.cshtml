        @Html.Partial("_FMIEduPartial")

        @Html.Partial("_ContactPartial")

        @Html.Partial("_HighSchoolEduPartial")

        @Html.Partial("_SocialLinksPartial")

        @Html.Partial("_AdditionalPartial")

        @Html.Partial("../Language/_LanguagesPartial")

        @Html.Partial("_InterestsPartial")

    </div>
    <!-- ENDS Filter container -->

</div>
